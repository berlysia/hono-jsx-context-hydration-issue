    "@hono/vite-ssg": "^0.1.0",
    "gh-pages": "^6.2.0",
    "prettier": "^3.4.2",
    "serve": "^14.2.4",
    "vite": "^5.2.12",
    "wrangler": "^3.96.0"
  }
}
